// Place your key bindings in this file to overwrite the defaults
[
	{
		"key": "ctrl+1",
		"command": "-workbench.action.focusFirstEditorGroup"
	},
	{
		"key": "ctrl+2",
		"command": "workbench.action.toggleActivityBarVisibility"
	},
	{
		"key": "ctrl+2",
		"command": "-workbench.action.focusSecondEditorGroup"
	},
	{
		"key": "ctrl+1",
		"command": "workbench.action.toggleMenuBar"
	},
	{
		"key": "alt+j",
		"command": "workbench.action.nextEditor"
	},
	{
		"key": "ctrl+pagedown",
		"command": "-workbench.action.nextEditor"
	},
	{
		"key": "alt+k",
		"command": "workbench.action.previousEditor"
	},
	{
		"key": "ctrl+pageup",
		"command": "-workbench.action.previousEditor"
	},
	{
		"key": "alt+k",
		"command": "-keybindings.editor.recordSearchKeys",
		"when": "inKeybindings && inKeybindingsSearch"
	},
	{
		"key": "alt+n",
		"command": "-C_Cpp.Navigate",
		"when": "editorTextFocus && editorLangId == 'c'"
	},
	{
		"key": "alt+n",
		"command": "-C_Cpp.Navigate",
		"when": "editorTextFocus && editorLangId == 'cpp'"
	},
	{
		"key": "alt+n",
		"command": "workbench.action.closeActiveEditor"
	},
	{
		"key": "ctrl+w",
		"command": "-workbench.action.closeActiveEditor"
	},
	{
		"key": "alt+u",
		"command": "editor.action.goToDeclaration",
		"when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
	},
	{
		"key": "f12",
		"command": "-editor.action.goToDeclaration",
		"when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
	},
	{
		"key": "ctrl+f12",
		"command": "-editor.action.goToImplementation",
		"when": "editorHasImplementationProvider && editorTextFocus && !isInEmbeddedEditor"
	},
	{
		"key": "ctrl+f12",
		"command": "-C_Cpp.GoToDeclaration",
		"when": "editorTextFocus && editorLangId == 'c'"
	},
	{
		"key": "alt+s",
		"command": "workbench.action.findInFiles"
	},
	{
		"key": "ctrl+shift+f",
		"command": "-workbench.action.findInFiles"
	},
	{
		"key": "ctrl+4",
		"command": "-workbench.action.focusFourthEditorGroup"
	},
	{
		"key": "ctrl+4",
		"command": "workbench.action.togglePanel"
	},
	{
		"key": "ctrl+j",
		"command": "-workbench.action.togglePanel"
	},
	{
		"key": "ctrl+3",
		"command": "-workbench.action.focusThirdEditorGroup"
	},
	{
		"key": "ctrl+3",
		"command": "workbench.action.toggleSidebarVisibility"
	},
	{
		"key": "ctrl+b",
		"command": "-workbench.action.toggleSidebarVisibility"
	},
	{
		"key": "alt+h",
		"command": "workbench.action.focusLeftGroup"
	},
	{
		"key": "ctrl+k ctrl+left",
		"command": "-workbench.action.focusLeftGroup"
	},
	{
		"key": "ctrl+k ctrl+right",
		"command": "-workbench.action.focusRightGroup"
	},
	{
		"key": "alt+p",
		"command": "-keybindings.editor.toggleSortByPrecedence",
		"when": "inKeybindings"
	},
	{
		"key": "alt+,",
		"command": "workbench.action.moveEditorLeftInGroup"
	},
	{
		"key": "ctrl+shift+pageup",
		"command": "-workbench.action.moveEditorLeftInGroup"
	},
	{
		"key": "alt+.",
		"command": "workbench.action.moveEditorRightInGroup"
	},
	{
		"key": "ctrl+shift+pagedown",
		"command": "-workbench.action.moveEditorRightInGroup"
	},
	{
		"key": "alt+w",
		"command": "-workbench.action.terminal.toggleFindWholeWord",
		"when": "terminalFindWidgetFocused"
	},
	{
		"key": "alt+w",
		"command": "-workbench.action.terminal.toggleFindWholeWordTerminalFocus",
		"when": "terminalFocus"
	},
	{
		"key": "alt+w",
		"command": "-toggleFindWholeWord",
		"when": "editorFocus"
	},
	{
		"key": "alt+w",
		"command": "-toggleSearchWholeWord",
		"when": "searchViewletFocus && searchViewletVisible"
	},
	{
		"key": "alt+e",
		"command": "workbench.action.focusActiveEditorGroup"
	},
  {
		"key": "f5",
		"command": "-workbench.action.debug.start",
		"when": "!inDebugMode"
	},
	{
		"key": "ctrl+shift+l",
		"command": "-editor.action.selectHighlights",
		"when": "editorFocus"
	},
	{
		"key": "alt+enter",
		"command": "-editor.action.selectAllMatches",
		"when": "editorFocus && findWidgetVisible"
	},
	{
		"key": "alt+enter",
		"command": "-debug.openBreakpointToSide",
		"when": "breakpointsFocused"
	},
	{
		"key": "ctrl+f12",
		"command": "-C_Cpp.GoToDeclaration",
		"when": "editorTextFocus && editorLangId == 'cpp'"
	},
	{
		"key": "f12",
		"command": "-editor.action.revealDefinition",
		"when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
	},
	{
		"key": "alt+/",
		"command": "editor.action.jumpToBracket",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+shift+\\",
		"command": "-editor.action.jumpToBracket",
		"when": "editorTextFocus"
	},
	{
		"key": "alt+c",
		"command": "-workbench.action.terminal.toggleFindCaseSensitive",
		"when": "terminalFindWidgetFocused"
	},
	{
		"key": "alt+c",
		"command": "-workbench.action.terminal.toggleFindCaseSensitiveTerminalFocus",
		"when": "terminalFocus"
	},
	{
		"key": "alt+c",
		"command": "-toggleFindCaseSensitive",
		"when": "editorFocus"
	},
	{
		"key": "alt+c",
		"command": "-toggleSearchCaseSensitive",
		"when": "searchViewletFocus && searchViewletVisible && !fileMatchOrFolderMatchFocus"
	},
	{
		"key": "alt+c",
		"command": "editor.action.commentLine",
		"when": "editorTextFocus && !editorReadonly"
	},
	{
		"key": "ctrl+/",
		"command": "-editor.action.commentLine",
		"when": "editorTextFocus && !editorReadonly"
	},
	{
		"key": "alt+o",
		"command": "-C_Cpp.SwitchHeaderSource",
		"when": "editorTextFocus && editorLangId == 'cpp'"
	},
	{
		"key": "alt+o",
		"command": "-C_Cpp.SwitchHeaderSource",
		"when": "editorTextFocus && editorLangId == 'c'"
	},
	{
		"key": "alt+p",
		"command": "workbench.action.keepEditor"
	},
	{
		"key": "ctrl+k enter",
		"command": "-workbench.action.keepEditor"
	},
	{
		"key": "alt+d",
		"command": "workbench.action.gotoSymbol"
	},
	{
		"key": "ctrl+shift+o",
		"command": "-workbench.action.gotoSymbol"
	},
	{
		"key": "alt+a",
		"command": "editor.action.peekDefinition",
		"when": "editorHasDefinitionProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
	},
	{
		"key": "ctrl+shift+f10",
		"command": "-editor.action.peekDefinition",
		"when": "editorHasDefinitionProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
	},
	{
		"key": "alt+pagedown",
		"command": "-scrollPageDown",
		"when": "textInputFocus"
	},
	{
		"key": "alt+i",
		"command": "cursorMove",
		 "args": {
			  "to": "up",
			  "by": "line",
			  "value": 10
		 },
		"when": "editorTextFocus"
	},
	{
		"key": "alt+pageup",
		"command": "-scrollPageUp",
		"when": "textInputFocus"
	},
	{
		"key": "alt+r",
		"command": "-workbench.action.terminal.toggleFindRegexTerminalFocus",
		"when": "terminalFocus"
	},
	{
		"key": "alt+r",
		"command": "-workbench.action.terminal.toggleFindRegex",
		"when": "terminalFindWidgetFocused"
	},
	{
		"key": "alt+r",
		"command": "-toggleFindRegex",
		"when": "editorFocus"
	},
	{
		"key": "alt+r",
		"command": "-toggleSearchRegex",
		"when": "searchViewletFocus && searchViewletVisible"
	},
	{
    "key": "alt+;",
    "command": "center-editor-window.center",
    "when": "editorTextFocus || findWidgetVisible"
  },
  {
    "key": "ctrl+l",
    "command": "-center-editor-window.center",
    "when": "editorTextFocus || findWidgetVisible"
	},
	{
		"key": "ctrl+x",
		"command": "-editor.action.clipboardCutAction",
		"when": "textInputFocus && !editorReadonly"
	},
	{
		"key": "ctrl+x",
		"command": "-extension.vim_ctrl+x",
		"when": "editorTextFocus && vim.active && vim.use<C-x> && !inDebugRepl"
	},
	{
		"key": "ctrl+x",
		"command": "-filesExplorer.cut",
		"when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !inputFocus"
	},
	{
		"key": "alt+x",
		"command": "workbench.action.splitEditorDown"
	},
	{
		"key": "alt+w",
		"command": "-workbench.action.terminal.toggleFindWholeWord",
		"when": "terminalFocus"
	},
	{
		"key": "alt+w",
		"command": "-toggleSearchEditorWholeWord",
		"when": "inSearchEditor && searchInputBoxFocus"
	},
	{
		"key": "alt+w",
		"command": "workbench.action.focusBelowGroup"
	},
	{
		"key": "ctrl+k ctrl+down",
		"command": "-workbench.action.focusBelowGroup"
	},
	{
		"key": "alt+o",
		"command": "workbench.action.quickOpen"
	},
	{
		"key": "ctrl+p",
		"command": "-workbench.action.quickOpen"
	},
	{
		"key": "alt+t",
		"command": "workbench.action.terminal.toggleTerminal"
	},
	{
		"key": "ctrl+`",
		"command": "-workbench.action.terminal.toggleTerminal"
	},
	{
		"key": "alt+r",
		"command": "workbench.action.navigateBack",
		"when": "editorFocus"
	},
	{
		"key": "ctrl+alt+-",
		"command": "-workbench.action.navigateBack"
	},
	{
		"key": "alt+l",
		"command": "-toggleFindInSelection",
		"when": "editorFocus"
	},
	{
		"key": "alt+l",
		"command": "-toggleSearchEditorContextLines",
		"when": "inSearchEditor"
	},
	{
		"key": "alt+l",
		"command": "eslint.executeAutofix"
	},
	{
		"key": "alt+f",
		"command": "editor.action.revealDefinition"
	}
]
