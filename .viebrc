set search=https://www.google.com/search?q=%s
set darkreader true
set darkreaderbg="#0f111b"
set darkreaderfg="#ecf0c1"
set firefoxmode="always"

nmap to <openNewTab><toExploreMode>
nmap tn d
nmap tj <nextTab>
nmap tk <previousTab>
nmap th <moveTabBackward>
nmap tl <moveTabForward>
nmap tp <openNewTab><openFromClipboard>
unmap t
unmap gt
unmap gT

nmap gb <backInHistory>
nmap ge <toExploreMode><End>
nmap gj <scrollPageDownHalf>
nmap gk <scrollPageUpHalf>

nmap o <toExploreMode>
