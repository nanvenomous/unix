[alias]
	b = branch
	ch = checkout
	c = commit
	ca = commit --amend
	s = status
	alias = config --get-regexp alias
	a = add
	pho = push origin
	pl = pull
	l = log
	cp = cherry-pick
	i = init
	rst = reset
	ph = push
	t = ls-tree -r HEAD --name-only
	sh = stash
	aa = add .
	lv = !git --no-pager log | nvim -
	bv = !git branch | nvim -
